package junit.model;

import static org.junit.Assert.*;

import org.junit.Test;

import editorexample.model.GSSTableColumnInfo;
import editorexample.model.GSSTableItemInfo;

public class GSSTableColumnInfoTest {

	@Test
	public void testToString() {
		GSSTableColumnInfo columnInfo = new GSSTableColumnInfo();
		assertEquals("", columnInfo.toString());
		GSSTableItemInfo itemInfo = new GSSTableItemInfo();
		itemInfo.setValue("name");
		columnInfo.putItem("name", itemInfo);
		assertEquals("\t\t<column name=\"name\"/>", columnInfo.toString());
		System.out.println(columnInfo.toString());
		GSSTableItemInfo itemInfo2 = new GSSTableItemInfo();
		itemInfo2.setValue("NVARCHAR");
		columnInfo.putItem("type", itemInfo2);
		assertEquals("\t\t<column name=\"name\" type=\"NVARCHAR\"/>", columnInfo.toString());
		System.out.println(columnInfo.toString());
	}

}
